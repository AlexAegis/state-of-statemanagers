		"ts-node": "~8.10.2",
		"tslint": "~6.1.2",
		"eslint": "~7.4.0",
		"typescript": "~3.9.6",
		"prettier": "~2.0.5",
		"@ngxs/cli": "~2.0.0",
		"@nrwl/nest": "~9.5.1",
		"@ngrx/store-devtools": "~9.2.0",
		"@angular/compiler-cli": "~10.0.3",
		"@angular/language-service": "~10.0.3",
		"@angular-devkit/build-angular": "~0.1000.1",
		"codelyzer": "~6.0.0",
		"jest-preset-angular": "~8.2.1",
		"@nrwl/jest": "~9.5.1",
		"jest": "~26.1.0",
