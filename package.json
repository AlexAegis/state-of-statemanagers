		"@ngxs/form-plugin": "~3.6.2",
		"@ngxs/router-plugin": "~3.6.2",
		"@ngxs/storage-plugin": "~3.6.2",
		"ngrx-store-localstorage": "~9.0.0",
		"rxjs": "~6.6.0",
		"zone.js": "~0.10.3",
		"@nestjs/common": "~7.3.2",
		"@nestjs/core": "~7.3.2",
		"@nestjs/platform-express": "~7.3.2",
		"reflect-metadata": "~0.1.13"
